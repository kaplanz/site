---
import Base from "../layouts/base.astro";

import { Markdown } from "@astropub/md";

import { title, about } from "../consts";

const request = await fetch(
  "https://github.com/kaplanz/kaplanz/raw/refs/heads/main/README.md",
);
const content = await request.text();
---

<Base {title} {about}>
  <Markdown of={content}/>
</Base>
